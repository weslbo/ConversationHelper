{
    "name": "AI conversation generator",
    "version": "1.0.0",
    "description": "AI conversation generator",
    "manifest_version": 3,
    "author": "Wesley De Bolster",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "AI conversation generator",
        "default_icon": "icon.png"
    }
}